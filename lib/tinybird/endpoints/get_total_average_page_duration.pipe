VERSION 2

NODE endpoint
SQL >
    %
    SELECT
        pageNumber,
        AVG(duration) AS avg_duration
    FROM
        page_views__v1
    WHERE
        documentId = {{ String(documentId, required=True) }}
        AND time >= {{ Int64(since, required=True) }}
        AND linkId NOT IN {{Array(linkId,String)}}
        AND viewId NOT IN {{Array(viewId,String)}}
    GROUP BY
        pageNumber
    ORDER BY
        pageNumber ASC
